<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="680.0" prefWidth="980.0" styleClass="parent" stylesheets="@../style.css"
            xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="client.gui.MainController">
    <Pane layoutX="2.0" layoutY="140.0" prefHeight="400.0" prefWidth="980.0" styleClass="pane"
          stylesheets="@../style.css">
        <Label layoutX="21.0" layoutY="98.0" text="Noch kein Account?" textFill="WHITE">
            <font>
                <Font size="20.0"/>
            </font>
        </Label>
        <Label layoutX="21.0" layoutY="134.0" prefHeight="64.0" prefWidth="222.0" text="Registriere dich jetzt!"
               textFill="WHITE" wrapText="true">
            <font>
                <Font size="16.0"/>
            </font>
        </Label>
        <Button layoutX="21.0" layoutY="200.0" mnemonicParsing="false" onAction="#SignUp_Modal" prefHeight="32.0"
                prefWidth="93.0" styleClass="trans-btn-white-border" text="Sign Up"/>
        <Label layoutX="655.0" layoutY="53.0" text="Du hast schon einen Account?" textAlignment="RIGHT"
               textFill="WHITE">
            <font>
                <Font size="20.0"/>
            </font>
        </Label>
        <Label alignment="CENTER_RIGHT" layoutX="695.0" layoutY="81.0" prefHeight="64.0" prefWidth="222.0"
               text="Melde dich hier an." textAlignment="RIGHT" textFill="WHITE" wrapText="true">
            <font>
                <Font size="16.0"/>
            </font>
        </Label>
        <Button layoutX="828.0" layoutY="150.0" mnemonicParsing="false" onAction="#SignIn_Modal" prefHeight="32.0"
                prefWidth="93.0" styleClass="trans-btn-white-border" text="Sign In"/>
        <VBox id="vbox" fx:id="vbox" layoutX="21.0" layoutY="-49.0" prefHeight="480.0" prefWidth="480.0"
              styleClass="white-pane"/>
    </Pane>
</AnchorPane>
