<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.app.ChatController">
   <children>
      <MenuBar VBox.vgrow="NEVER">
         <menus>
            <Menu mnemonicParsing="false" text="File">
               <items>
                  <MenuItem mnemonicParsing="false" text="New" />
                  <MenuItem mnemonicParsing="false" text="Open…" />
                  <Menu mnemonicParsing="false" text="Open Recent" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem mnemonicParsing="false" text="Close" />
                  <MenuItem mnemonicParsing="false" text="Save" />
                  <MenuItem mnemonicParsing="false" text="Save As…" />
                  <MenuItem mnemonicParsing="false" text="Revert" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem mnemonicParsing="false" text="Preferences…" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem mnemonicParsing="false" text="Quit" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Edit">
               <items>
                  <MenuItem mnemonicParsing="false" text="Undo" />
                  <MenuItem mnemonicParsing="false" text="Redo" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem mnemonicParsing="false" text="Cut" />
                  <MenuItem mnemonicParsing="false" text="Copy" />
                  <MenuItem mnemonicParsing="false" text="Paste" />
                  <MenuItem mnemonicParsing="false" text="Delete" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem mnemonicParsing="false" text="Select All" />
                  <MenuItem mnemonicParsing="false" text="Unselect All" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
               <items>
                  <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS" >
         <children>
            <Button fx:id="login_button" layoutX="346.0" layoutY="30.0" mnemonicParsing="false" text="login" onAction="#login"/>
            <TextField fx:id="login_email" layoutX="22.0" layoutY="30.0" promptText="email" />
            <TextField fx:id="login_password" accessibleRole="PASSWORD_FIELD" layoutX="187.0" layoutY="30.0" promptText="password" />
            <TextField fx:id="register_email" layoutX="23.0" layoutY="65.0" promptText="email" />
            <TextField fx:id="register_password" accessibleRole="PASSWORD_FIELD" layoutX="187.0" layoutY="65.0" promptText="password" />
            <TextField fx:id="register_nickname" layoutX="346.0" layoutY="65.0" promptText="nickname" />
            <Button fx:id="register_button" layoutX="505.0" layoutY="65.0" mnemonicParsing="false" text="register" />
            <Button fx:id="joinGroup_button" layoutX="187.0" layoutY="100.0" mnemonicParsing="false" text="joinGroup" />
            <TextField fx:id="joinGroup_id" layoutX="22.0" layoutY="100.0" promptText="group id" />
            <Button fx:id="getChannelMembers_button" layoutX="187.0" layoutY="136.0" mnemonicParsing="false" text="getChannelmembers" />
            <TextField fx:id="getChannelMembers_id" layoutX="22.0" layoutY="136.0" promptText="group id" />
            <Text fx:id="login_text" layoutX="398.0" layoutY="47.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" visible="true" />
            <Text fx:id="register_text" layoutX="568.0" layoutY="82.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
            <Text fx:id="joinGroup_text" layoutX="262.0" layoutY="117.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
            <Text fx:id="getChannelMembers_text" layoutX="320.0" layoutY="153.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
            <Button fx:id="addFriend_button" layoutX="187.0" layoutY="174.0" mnemonicParsing="false" text="ADD FRIEND" />
            <TextField fx:id="addFriend_id" layoutX="22.0" layoutY="174.0" promptText="friend id" />
            <Text fx:id="addFriend_text" layoutX="284.0" layoutY="191.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
            <TextField fx:id="sendMessage_message" layoutX="23.0" layoutY="207.0" promptText="message" />
            <Button fx:id="sendMessage_button" layoutX="337.0" layoutY="207.0" mnemonicParsing="false" text="send message" />
            <TextField fx:id="sendMessage_user" layoutX="182.0" layoutY="207.0" promptText="user" />
            <Text fx:id="sendMessage_text" layoutX="436.0" layoutY="224.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
         </children>
      </AnchorPane>
   </children>
</VBox>
